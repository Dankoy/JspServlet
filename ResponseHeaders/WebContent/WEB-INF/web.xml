<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ResponseHeaders</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>applesexcel</servlet-name>
    <servlet-class>ApplesAndOranges</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>applesexcel</servlet-name>
    <url-pattern>/aao</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>refreshme</servlet-name>
  	<servlet-class>ReloadingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>refreshme</servlet-name>
  	<url-pattern>/refreshing-page</url-pattern>
  </servlet-mapping>
  
  
</web-app>