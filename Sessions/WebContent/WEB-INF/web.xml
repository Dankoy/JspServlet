<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Sessions</display-name>
  <welcome-file-list>
    <welcome-file>index-core.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>show-session</servlet-name>
    <servlet-class>coresession.ShowSession</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>show-session</servlet-name>
    <url-pattern>/show-session-info</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>basket</servlet-name>
    <servlet-class>coresession.ShowItems</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>basket</servlet-name>
    <url-pattern>/show-items</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>repeated-visitor</servlet-name>
  	<servlet-class>coresession.RepeatedVisitor</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>repeated-visitor</servlet-name>
  	<url-pattern>/repeated-visitor</url-pattern>
  </servlet-mapping>
</web-app>